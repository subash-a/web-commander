{
    "manifest_version": 2,
    "name": "MyHome",
    "description": "Just a starter extension for commands",
    "version": "1.0",
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "content_scripts": [],
    "permissions": [
	"activeTab",
	"https://ajax.googleapis.com/"
    ],
    "commands": {
	"_execute_browser_action": {
	    "suggested_key": {
		"default": "Ctrl+U",
		"mac": "Command+U"
	    }
	}
    }
}
